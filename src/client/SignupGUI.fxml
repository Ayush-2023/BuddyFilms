<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="246.0" prefWidth="331.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.SignupGUI">
   <children>
      <Button fx:id="signupButton" layoutX="132.0" layoutY="179.0" mnemonicParsing="false" onAction="#signupListener" text="Signup" />
      <Label layoutX="33.0" layoutY="74.0" text="Username" />
      <Label layoutX="33.0" layoutY="106.0" text="Password" />
      <Label layoutX="33.0" layoutY="140.0" text="Retype Password" />
      <TextField fx:id="usernameField" layoutX="142.0" layoutY="70.0" promptText="Enter Username" />
      <Label layoutX="73.0" layoutY="22.0" text="Buddy Films Signup">
         <font>
            <Font name="Book Antiqua Bold" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="142.0" layoutY="102.0" promptText="Enter Password" />
      <PasswordField fx:id="confirmPasswordField" layoutX="142.0" layoutY="136.0" promptText="Renter Password" />
   </children>
</AnchorPane>
